<?xml version='1.0'?>
<robot name="right_turn">
  <link name='stright'>
    <visual name="visual">
      <geometry>
        <box size="4 1 1"/>  <!-- size of the first block -->
      </geometry>
      <material name="orange">
        <color rgba="1.0 0.5 0.0 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="4 1 1"/>  <!-- size of the first block -->
      </geometry>
    </collision>
    <inertial>
      <mass value="1.0"/>  <!-- mass of the first block -->
      <origin xyz="0 0 0"/>
      <inertia ixx="0.0025" ixy="0.0" ixz="0.0" iyy="0.0025" iyz="0.0" izz="0.0025"/> <!-- simple inertia for a block -->
    </inertial>
  </link>
    
  <link name='stright_1'>
    <visual name="visual">
      <geometry>
        <box size="4 1 1"/>  <!-- size of the first block -->
      </geometry>
      <material name="orange">
        <color rgba="1.0 0.5 0.0 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="4 1 1"/>  <!-- size of the first block -->
      </geometry>
    </collision>
    <inertial>
      <mass value="1.0"/>  <!-- mass of the first block -->
      <origin xyz="0 0 0"/>
      <inertia ixx="0.0025" ixy="0.0" ixz="0.0" iyy="0.0025" iyz="0.0" izz="0.0025"/> <!-- simple inertia for a block -->
    </inertial>
  </link>

  <link name='horizontal'>
    <visual name="visual">
      <geometry>
        <box size="1 4.5 1"/>  <!-- size of the first block -->
      </geometry>
      <material name="orange">
        <color rgba="1.0 0.5 0.0 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="1 4.5 1"/>  <!-- size of the first block -->
      </geometry>
    </collision>
    <inertial>
      <mass value="1.0"/>  <!-- mass of the first block -->
      <origin xyz="0 0 0"/>
      <inertia ixx="0.0025" ixy="0.0" ixz="0.0" iyy="0.0025" iyz="0.0" izz="0.0025"/> <!-- simple inertia for a block -->
    </inertial>
  </link>

  <joint name="joint_1" type="fixed">
    <parent link="horizontal"/>
    <child link="stright_1"/>
    <origin xyz="2.5 1.75 0"/>  <!-- Position of the second block relative to the first block -->
  </joint>

  <joint name="joint_2" type="fixed">
    <parent link="horizontal"/>
    <child link="stright"/>
    <origin xyz="2.5 -1.75 0"/>  <!-- Position of the second block relative to the first block -->
  </joint>

</robot>